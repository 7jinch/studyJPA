package jpa.basic;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.*;

@Entity
public class Team {
	@Id
	@GeneratedValue
	@Column(name = "TEAM_ID")
	private Long id;
	private String name;
	
	@OneToMany(mappedBy = "") // 일대아의 관계임을 알려줌(team이 "1", member가 "다")
	private List<Member> member = new ArrayList<>();
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
